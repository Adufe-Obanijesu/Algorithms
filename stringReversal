function stringReversal(string) {
    return string.split("").reverse().join("");
}

function _stringReversal(string) {
    let stringArray = [...string];
    let reversedString = "";
    for(let i=0; i<stringArray.length; i++) {
        reversedString = stringArray[i] + reversedString;
    }
    return reversedString;
}

function __stringReversal(string) {
    let stringArray = [...string];
    let reversedString = [];
    for(let i=stringArray.length; i>=0; i--) {
        reversedString.push(stringArray[i]);
    }
    reversedString = reversedString.join("");
    return reversedString;
}

function ___stringReversal(string) {
    let reversedString = "";
    for(let character of string) reversedString = character + reversedString;
    return reversedString;
}

stringReversal("Hello world");
_stringReversal("Hello world");
__stringReversal("Hello world");
___stringReversal("Hello world");